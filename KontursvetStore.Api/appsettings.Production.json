{
  "Serilog": {
    "Using":  [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": { "path":  "./Logs/log-.txt", "rollingInterval": "Day" }
      }
    ]
  },
  "ConnectionStrings": {
    "DefaultConnection": "Host=postgres_container;Port=5432;Database=store;Username=postgres;Password=postgres"
  }
}